{% load staticfiles %}
<html lang="ru">
<head>
  <title>{% block title %}Заголовок{% endblock %}</title>
  <link rel="stylesheet" href="{% static 'styles.css' %}" type="text/css" />

  <link href="http://vjs.zencdn.net/c/video-js.css" rel="stylesheet">
  <script src="http://vjs.zencdn.net/c/video.js"></script>
  <script src="http://cdn.sockjs.org/sockjs-0.3.2.min.js"></script>

</head>

<body>
   <table id="head" width="100%" height="10%" border="2">
    <tr>
     <td>{% include 'head.html' %}</td> 
    </tr>
   </table> 


   <table id="con" width="100%" height="85%" border="2">
    <tr>
     <td id="menu" width="10%">{% include 'menu.html' %}</td>
     <td id="content" width="90%">{% include 'content.html' %}</td>
    </tr> 
   </table> 

   <table id="foot" width="100%" height="5%" border="2">
    <tr>
     <td>{% block foot %}Foot !!!!!!!!!!{% endblock %}</td> 
    </tr>
   </table> 

   <script src="{% static 'scripts.js' %}"></script>
    
</body>


</html>
